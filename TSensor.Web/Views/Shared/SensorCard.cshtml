@model TSensor.Web.Models.Entity.SensorValue

<div class="t-cell" data-sensorguid="@Model.SensorGuid">
    <h5 class="t-info">
        <a href="javascript:void(0)" class="t-tank-setsensor" data-device="@Model.DeviceGuid" data-izk="@Model.IzkNumber" data-sensor="@Model.SensorSerial">
            <span title="Идентификатор устройства">@Model.DeviceGuid</span>
            <span title="Идентификатор ИЗК">@Model.IzkNumber</span>
            <span title="Идентификатор датчика">@Model.SensorSerial</span>
        </a>
    </h5>
    <h6><span class="t-date">@Model.InsertDateStr</span></h6>
    <div>
        <div class="bl2">
            <div>
                <div class="tank-level" style="height: @Model.PercentLevel%"></div>
            </div>
        </div>
        <div class="bl3">
            <span><span class="t-label t-img-weight">Масса</span><span class="t-liquidEnvironmentLevel">@Model.LiquidEnvironmentLevel.ToString().Replace(",", ".")</span> т</span>
            <span><span class="t-label t-img-volume">Объём</span><span class="t-environmentVolume">@Model.EnvironmentVolume.ToString().Replace(",", ".")</span> м³</span>
            <span><span class="t-label t-img-density">Плотность</span><span class="t-liquidDensity">@Model.LiquidDensity.ToString().Replace(",", ".")</span> кг/м³</span>
            <span><span class="t-label t-img-temperature">Температура</span><span class="t-avgT">@Model.AvgT.ToString().Replace(",", ".")</span> °C</span>
            <span><span class="t-label t-img-level">Уровень</span><span class="t-environmentLevel">@Model.EnvironmentLevel.ToString().Replace(",", ".")</span> мм</span>
        </div>
        <div class="bl-error"></div>
    </div>
</div>