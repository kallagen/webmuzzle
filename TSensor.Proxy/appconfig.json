{
  // идентификатор устройства
  "deviceGuid": "00001",

  // режим логирования error или debug
  "loggerType": "debug",
  // максимальный размер файла с логами, кбайты
  "maxErrorFileSize": 1000,

  // источник получения данных tcp или com
  "inputMode": "com",
  // список портов для получения данных
  // если необходимо использовать все доступные порты в com режиме, то оставить пустой массив "portList": [ ]
  // пример в com режиме "portList": [
  // "/dev/ttyUSB16",
  // "COM3" ],
  // в tcp режиме можно указать в том числе интервал
  // пример в tcp режиме "portList": [
  // 8081,
  // "8082",
  // "8083-8085" ],
  "portList": [
    "COM4"
  ],

  // цель для обработанных данных api или file
  "outputMode": "api",
  // адрес api
  //  "apiHost": "80.76.40.56:8081", TODO вернуть на нормальный "80.76.40.56:8081"
  //машина для тестов где все настроено 109.167.253.104:4652
  "apiHost": "127.0.0.1:5000",

  // папка для архивных данных (если режим file или есть проблемы при доставке сообщений через api)
  "archiveFolder": "/home/pi/tsensor/archived",
  // максимальный размер файла с данными в режиме file (а также архивными в режиме api), кбайты
  "maxArchiveFileSize": 100,
  // интервал рассылки архивных данных в режиме api, секунды
  "archiveSendInterval": 300,

  // устройство с gps для получения координат, пример: /dev/ttyS0
  // если gps не установлен, то оставить пустым //"/dev/ttyS0"
  "gpsDevice": "",
  // время обновления координат, секунды
  "gpsSendInterval": 20,
  // время запроса о поступлении новых команд, секунды
  "commandGetInterval": 10
}